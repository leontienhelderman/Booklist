@model IEnumerable<BooklistLib.Models.BookModel>;

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<div>
    <table>
        <thead>
            <tr>
                <th>Title</th>
                <th>Author</th>
                <th>Genre</th>
                <th>Rating</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var book in Model)
            {
                <tr>
                    <td>@book.Title</td>
                    <td>@book.Author</td>
                    <td>@book.Genre</td>
                    <td>@book.Rating</td>
                </tr>
            }
        </tbody>
    </table>
</div>